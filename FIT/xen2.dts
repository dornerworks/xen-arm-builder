/dts-v1/;

/ {
	#address-cells = <0x1>;
	#size-cells = <0x1>;
	interrupt-parent = <0x1>;
	model = "Cubietech Cubietruck";
	compatible = "cubietech,cubietruck", "allwinner,sun7i-a20";

	chosen {
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		xen,xen-bootargs = "console=dtuart dtuart=/soc@01c00000/serial@01c28000 dom0_mem=512M,max:512M";

		dom0 {
			compatible = "xen,linux-zimage", "xen,multiboot-module";
			reg = <0xaf600000 0x800000>;
			bootargs = "console=hvc0 ro root=/dev/mmcblk0p2 rootwait clk_ignore_unused another_config";
		};
	};

	aliases {
		ethernet0 = "/soc@01c00000/ethernet@01c50000";
		serial0 = "/soc@01c00000/serial@01c28000";
		serial1 = "/soc@01c00000/serial@01c28400";
		serial2 = "/soc@01c00000/serial@01c28800";
		serial3 = "/soc@01c00000/serial@01c28c00";
		serial4 = "/soc@01c00000/serial@01c29000";
		serial5 = "/soc@01c00000/serial@01c29400";
		serial6 = "/soc@01c00000/serial@01c29800";
		serial7 = "/soc@01c00000/serial@01c29c00";
	};

	memory {
		device_type = "memory";
		reg = <0x40000000 0x80000000>;
	};

	cpus {
		#address-cells = <0x1>;
		#size-cells = <0x0>;

		cpu@0 {
			compatible = "arm,cortex-a7";
			device_type = "cpu";
			reg = <0x0>;
		};

		cpu@1 {
			compatible = "arm,cortex-a7";
			device_type = "cpu";
			reg = <0x1>;
		};
	};

	timer {
		compatible = "arm,armv7-timer";
		interrupts = <0x1 0xd 0xf08 0x1 0xe 0xf08 0x1 0xb 0xf08 0x1 0xa 0xf08>;
	};

	pmu {
		compatible = "arm,cortex-a7-pmu", "arm,cortex-a15-pmu";
		interrupts = <0x0 0x78 0x4 0x0 0x79 0x4>;
	};

